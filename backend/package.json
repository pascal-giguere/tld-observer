{
  "name": "@tld-observer/backend",
  "private": true,
  "description": "TLD Observer backend-end",
  "version": "1.0.0",
  "scripts": {
    "start:clock": "yarn build && yarn serve:clock",
    "start:web": "yarn build && yarn serve:web",
