  },
  "dependencies": {
    "express": "^4.17.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.14",
    "prettier": "2.0.5"
  }
}
